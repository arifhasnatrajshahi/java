    private void jTable1MousePressed(java.awt.event.MouseEvent evt) {                                     
         int row=jTable1.getSelectedRow();
        String ptr=jTable1.getModel().getValueAt(0, row).toString();
        
        
        try {
            String sql="select * from CSTE where Name='"+ptr+"'";
            pst=conn.prepareStatement(sql);
            rs=pst.executeQuery();
            if(rs.next()){
            jLabel1.setText(rs.getString("id"));
            jLabel6.setText(rs.getString("Name"));
            jLabel7.setText(rs.getString("Address"));
             jLabel8.setText(rs.getString("Status"));
               jLabel9.setText(rs.getString("Phone"));
            jLabel10.setText(rs.getString("Email"));
            
             
                 byte[] imagedata=rs.getBytes("Photo");
                 format =new ImageIcon(imagedata);
                /* int hig=format.getIconHeight();
                 int wid=format.getIconWidth();
                 String h=Integer.toString(hig);
                 String w=Integer.toString(wid);
                 if(hig>300){
                 
                 
                 }
                 jLabel11.setText(h);
                 jLabel11.setIcon(format);
                 //jLabel11.setMaximumSize(3);
            
            }*/
            }
        } catch (Exception e) {
            JOptionPane.showMessageDialog(null, e);
        }
    }                                    

